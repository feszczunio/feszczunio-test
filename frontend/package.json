{
  "name": "frontend",
  "version": "6.7.0",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Statik",
  "homepage": "https://github.com/statik-space/statik-boilerplate",
  "repository": "git://github.com/statik-space/statik-boilerplate.git",
  "license": "GPL-3.0-or-later",
  "author": "Statik Team <hello@statik.space>",
  "scripts": {
    "clean": "gatsby clean",
    "serve": "gatsby serve --prefix-paths",
    "build": "gatsby build --prefix-paths --verbose --log-pages --no-color",
    "build:local": "GATSBY_ACTIVE_ENV=local yarn build",
    "build:dev": "GATSBY_ACTIVE_ENV=dev yarn build",
    "build:stg": "GATSBY_ACTIVE_ENV=stg yarn build",
    "build:prd": "GATSBY_ACTIVE_ENV=prd yarn build",
    "build:debug": "node --inspect-brk $(yarn bin gatsby) build --prefix-paths --verbose --log-pages",
    "develop": "gatsby develop --verbose",
    "develop:local": "GATSBY_ACTIVE_ENV=local yarn develop",
    "develop:dev": "GATSBY_ACTIVE_ENV=dev yarn develop",
    "develop:stg": "GATSBY_ACTIVE_ENV=stg yarn develop",
    "develop:prd": "GATSBY_ACTIVE_ENV=prd yarn develop",
    "develop:debug": "gatsby develop --inspect-brk --verbose",
    "build-diff": "rm -rf public-first-build && gatsby clean && gatsby build --no-uglify --verbose --log-pages && cp -r public public-first-build && gatsby build --no-uglify --verbose --log-pages && diff -u -r public-first-build public > build-diff.diff",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check --debug-check .",
    "build:save": "rm -rf .tmp && mkdir .tmp && cp -r public .tmp/public && cp -r .cache .tmp/.cache",
    "build:restore": "rm -rf public && rm -rf .cache && cp -r .tmp/public public && cp -r .tmp/.cache .cache"
  },
  "dependencies": {
    "@react-hook/window-scroll": "^1.3.0",
    "@statik-space/gatsby-blocks-statik": "^1.1.2",
    "@statik-space/gatsby-theme-statik": "^1.1.7",
    "clsx": "^1.1.1",
    "gatsby": "^4.23.1",
    "lodash": "^4.17.21",
    "normalize-url": "^7.0.3",
    "prop-types": "^15.7.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.2.0",
    "react-spinners": "^0.13.8",
    "react-transition-group": "^4.4.1",
    "rot-13": "^1.0.0",
    "universal-cookie": "^4.0.4",
    "xss": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.41",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^17.0.1",
    "async-retry": "^1.3.3",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby": "^2.4.0",
    "core-js": "^3.20.1",
    "flexsearch": "^0.7.31",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.30",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "p-limit": "^4.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
