{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "statik/accordion-item",
	"version": "4.18.1",
	"title": "Accordion Item",
	"parent": [ "statik/accordion" ],
	"category": "layout",
	"description": "A child item of an Accordion Block.",
	"keywords": [],
	"textdomain": "statik-blocks",
	"attributes": {
		"title": {
			"type": "string"
		},
		"defaultExpanded": {
			"type": "boolean",
			"default": false
		},
		"headerSelector": {
			"type": "string",
			"enum": [ "p", "span", "h1", "h2", "h3", "h4", "h5" ],
			"default": "h3"
		}
	},
	"supports": {
		"reusable": false,
		"html": false,
		"inserter": false
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
