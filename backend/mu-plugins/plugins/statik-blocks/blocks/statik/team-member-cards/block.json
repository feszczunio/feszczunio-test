{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "statik/team-member-cards",
	"version": "4.18.1",
	"title": "Team Member Cards",
	"category": "layout",
	"description": "Present Team Members as card components either organised as a rows and columns or a carousel.",
	"keywords": [],
	"textdomain": "statik-blocks",
	"attributes": {
		"cardsPerRow": {
			"type": "array",
			"items": {
				"type": "number"
			},
			"default": [ 3 ]
		},
		"displayImage": {
			"type": "boolean",
			"default": true
		},
		"displayName": {
			"type": "boolean",
			"default": true
		},
		"displayShortDescription": {
			"type": "boolean",
			"default": true
		},
		"displayLongDescription": {
			"type": "boolean",
			"default": false
		},
		"displaySocialMedia": {
			"type": "boolean",
			"default": true
		},
		"displayReadMoreButton": {
			"type": "boolean",
			"default": true
		},
		"followUpArea": {
			"type": "string",
			"enum": [ "none", "area", "button" ],
			"default": "none"
		},
		"followUpBehaviour": {
			"type": "string",
			"enum": [ "redirect", "modal" ],
			"default": "redirect"
		},
		"modalContent": {
			"type": "string",
			"enum": [ "content", "website" ],
			"default": "content"
		},
		"query": {
			"type": "object",
			"default": {}
		},
		"excludeCurrentPost": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {
		"query": {}
	},
	"supports": {
		"align": [ "wide", "full" ]
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"viewScript": "file:./build/view.js"
}
